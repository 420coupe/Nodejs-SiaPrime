{
  "name": "siaprime.js",
  "version": "0.0.6",
  "description": "Node wrapper for spd of the ScPrime network",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/420coupe/Nodejs-SiaPrime.git"
  },
  "main": "lib/siaprime.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "test": "npm run lint && mocha --compilers js:babel-register --recursive ./test",
    "prepublish": "npm run build",
    "lint": "eslint --max-warnings 0 ./src ./test",
    "kill": "killall spd; while pgrep spd; do sleep .5; done;",
    "clean": "npm run kill && rm -rf Sia node_modules"
  },
  "keywords": [
    "SiaPrime",
    "SiaPrime-UI",
    "Decentralized",
    "Cloud",
    "Storage",
    "Altcoin",
    "Cryptocurrency"
  ],
  "bugs": {
    "url": "https://github.com/420coupe/Nodejs-SiaPrime/issues"
  },
  "author": "Nebulous Labs",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/cli": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "chai": "^4.1.2",
    "eslint": "7.2.0",
    "mocha": "^7.0.1",
    "nock": "^9.0.18",
    "proxyquire": "^2.1.3",
    "readdir": "1.0.2",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "request": "^2.81.0"
  }
}
